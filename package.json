{
  "name": "lol-clubs-bot",
  "version": "0.0.1",
  "description": "Bot for clubs in league of legends",
  "author": "Antosik",
  "license": "MIT",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "prestart:prod": "rimraf dist && tsc",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "watch": "tsc -w"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "co": "^4.6.0",
    "date-fns": "^2.0.0-alpha.27",
    "discord.js": "^11.4.2"
  },
  "devDependencies": {
    "@types/node": "^10.9.4",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.11.0",
    "typescript": "^3.0.1"
  }
}
