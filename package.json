{
  "name": "lemi",
  "version": "1.0.0",
  "description": "Bot for Clubs in League of Legends",
  "scripts": {
    "start": "node dist/main.js",
    "build": "tsc --sourceMap false",
    "watch": "tsc -w",
    "postinstall": "npm run build",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "jest": "jest  --setupFiles dotenv/config",
    "test": "npm run lint && npm run jest"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "axios-extensions": "^3.0.5",
    "co": "^4.6.0",
    "date-fns": "^2.0.0-alpha.27",
    "discord.js": "^11.4.2",
    "dotenv": "^6.2.0"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/jest": "^23.3.13",
    "@types/nock": "^9.3.1",
    "@types/node": "^10.9.4",
    "jest": "^24.7.1",
    "nock": "^10.0.6",
    "nodemon": "^1.18.11",
    "rimraf": "^2.6.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "author": "Antosik",
  "license": "MIT",
  "repository": "Antosik/lemi",
  "homepage": "https://github.com/Antosik/lemi",
  "bugs": "https://github.com/Antosik/lemi/issues"
}
