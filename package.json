{
  "name": "lemi",
  "version": "1.0.2",
  "description": "Bot for Clubs in League of Legends",
  "scripts": {
    "start": "node dist/main.js",
    "build": "tsc --sourceMap false",
    "watch": "tsc -w",
    "postinstall": "npm run build",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "jest": "jest --setupFiles dotenv/config",
    "test": "npm run lint && npm run jest"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "axios-extensions": "^3.0.6",
    "co": "^4.6.0",
    "date-fns": "^2.9.0",
    "discord.js": "^11.5.1",
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^24.9.1",
    "@types/nock": "^11.1.0",
    "@types/node": "^10.17.14",
    "jest": "^24.9.0",
    "nock": "^11.7.2",
    "nodemon": "^2.0.2",
    "rimraf": "^3.0.1",
    "ts-jest": "^24.3.0",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "author": "Antosik",
  "license": "MIT",
  "repository": "Antosik/lemi",
  "homepage": "https://github.com/Antosik/lemi",
  "bugs": "https://github.com/Antosik/lemi/issues"
}
