{
  "name": "Lemi",
  "description": "Бот для Discord'a, позволяющий быстро получить информацию о Клубах в League of Legends",
  "repository": "https://github.com/Antosik/lemi",
  "env": {
    "LOL_TOKEN": {
      "description": "Token Discord'a (https://github.com/Antosik/lemi/wiki/Установка#1-Получение-tokena-lola)",
      "required": false
    },
    "DISCORD_TOKEN": {
      "description": "Token Discord'a (https://github.com/Antosik/lemi/wiki/Установка#2-Получение-tokena-discorda)",
      "required": true
    },
    "LEMI_PREFIX": {
      "description": "Префикс для команд",
      "value": "le!"
    }
  },
  "formation": {
    "web": {
      "quantity": 0,
      "size": "free"
    },
    "worker": {
      "quantity": 1,
      "size": "free"
    }
  },
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    }
  ],
  "keywords": ["discord", "bot", "league of legends"]
}